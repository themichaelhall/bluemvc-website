{
  "name": "bluemvc/bluemvc-website",
  "description": "Bare template for a website using the BlueMvc PHP framework",
  "homepage": "https://bluemvc.com/",
  "license": "Unlicense",
  "type": "project",
  "keywords": [
    "blue",
    "mvc",
    "framework",
    "php",
    "bare",
    "template",
    "website"
  ],
  "authors": [
    {
      "name": "Michael Hall",
      "homepage": "https://michaelhall.se/"
    }
  ],
  "require": {
    "php": ">=8.0",
    "bluemvc/bluemvc-core": "^3.0",
    "bluemvc/bluemvc-twig": "^3.0",
    "bluemvc/bluemvc-forms": "^3.0",
    "bluemvc/bluemvc-api": "^3.0",
    "bluemvc/bluemvc-fakes": "^3.0"
  },
  "autoload": {
    "psr-4": {
      "Application\\": "Application"
    }
  },
  "config": {
    "preferred-install": {
      "*": "dist"
    }
  }
}
